{
  "name": "geo-coding-challenge",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": "git@github.com:TierMobility/geo-coding-challenge.git",
  "author": "vpriem <vinzent.priem@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^16.x",
    "yarn": "^1.22.x"
  },
  "scripts": {
    "postinstall": "yarn husky install",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf {dist,tsconfig.tsbuildinfo,yarn-error.log,coverage}",
    "lint": "eslint --ext .ts ./src",
    "test": "jest",
    "format": "prettier --write src/{*.ts,**/*.ts}",
    "u": "yarn upgrade-interactive --latest",
    "up": "docker-compose run --service-ports code",
    "down": "docker-compose down",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/pg": "8.6.5",
    "pg": "8.7.3"
  },
  "devDependencies": {
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.10",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "@vpriem/geojson": "1.0.0",
    "devmoji": "2.3.0",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "lint-staged": "12.2.1",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.3",
    "typescript": "4.5.4"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
